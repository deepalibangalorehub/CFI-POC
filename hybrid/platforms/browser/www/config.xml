<?xml version='1.0' encoding='utf-8'?>
<widget id="org.oraclejet.cfidemo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cfidemo</name>
    <description>
        An Oracle JavaScript Extension Toolkit (JET) mobile app
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="scripts/hooks/jetAfterPrepare.js" type="after_prepare" />
</widget>
